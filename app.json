{
  "expo": {
    "name": "Nthome",
    "slug": "Nthome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0DCAF0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses background location to track your trip and provide real-time updates.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to track your trip and provide navigation.",
        "NSFaceIDUsageDescription": "This app uses Face ID or Touch ID to authenticate drivers before going online for security purposes."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCW_3yyD7H0125SOsP-vABHG6BrSm47j-w"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0DCAF0"
      },
      "package": "com.dj_kd.Nthome",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCW_3yyD7H0125SOsP-vABHG6BrSm47j-w"
        }
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow location app to use your location.",
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      "expo-router"
    ],
    "extra": {
      "googleMapsApiKey": "AIzaSyCW_3yyD7H0125SOsP-vABHG6BrSm47j-w",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0f34d97a-9a58-4bd0-a84f-42c50d3196e1"
      }
    },
    "newArchEnabled": true
  }
}
